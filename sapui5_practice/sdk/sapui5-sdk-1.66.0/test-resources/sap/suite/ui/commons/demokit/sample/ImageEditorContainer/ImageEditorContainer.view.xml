<mvc:View
	controllerName="sap.suite.ui.commons.sample.ImageEditorContainer.ImageEditorContainer"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.suite.ui.commons.imageeditor"
	width="100%"
	height="100%">
	<ImageEditorContainer id="editor">
		<ImageEditor id="image" />
		<customResizeItems>
			<CustomSizeItem width="1920" height="1200" label="1920 x 1200" relative="false"/>
			<CustomSizeItem width="1600" height="900" label="1600 x 900" relative="false"/>
			<CustomSizeItem width="1440" height="900" label="1440 x 900" relative="false"/>
			<CustomSizeItem width="1024" height="768" label="1024 x 768" relative="false"/>
			<CustomSizeItem width="2" height="2" label="double size" relative="true" icon="sap-icon://resize"/>
			<CustomSizeItem width="0.5" height="0.5" label="half size" relative="true" icon="sap-icon://resize"/>
			<CustomSizeItem width="2" height="1" label="make wider" relative="true" icon="sap-icon://resize"/>
			<CustomSizeItem width="1" height="2" label="make higher" relative="true" icon="sap-icon://resize"/>
		</customResizeItems>
		<customRectangleCropItems>
			<CustomSizeItem width="16" height="9" label="16:9" relative="true"/>
			<CustomSizeItem width="16" height="10" label="16:10" relative="true"/>
			<CustomSizeItem width="1" height="1" label="1:1" relative="true"/>
			<CustomSizeItem width="4" height="3" label="4:3" relative="true"/>
			<CustomSizeItem width="400" height="400" label="400 x 400" relative="false"/>
			<CustomSizeItem width="250" height="400" label="250 x 400" relative="false"/>
		</customRectangleCropItems>
		<customEllipseCropItems>
			<CustomSizeItem width="1" height="1" label="1:1" relative="true"/>
			<CustomSizeItem width="2" height="1" label="2:1" relative="true"/>
			<CustomSizeItem width="400" height="400" label="400 x 400" relative="false"/>
			<CustomSizeItem width="250" height="400" label="250 x 400" relative="false"/>
		</customEllipseCropItems>
	</ImageEditorContainer>
</mvc:View>
